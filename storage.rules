rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    function signedIn() { return request.auth != null; }
    match /incidents/{userId}/{allPaths=**} {
      allow read: if signedIn() && request.auth.uid == userId;
      allow write: if signedIn() && request.auth.uid == userId;
    }
  }
}

